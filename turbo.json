{
	"$schema": "https://turborepo.com/schema.json",
	"ui": "tui",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$"],
			"outputs": ["dist/**"]
		},
    "test": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", "**/*.test.ts"],
			"cache": true
		}
	}
}
